{
    "name": "database",
    "license": "MIT",
    "version": "0.0.0",
    "main": "./index.ts",
    "types": "./index.ts",
    "scripts": {
        "db:generate": "prisma generate",
        "db:push": "prisma db push --skip-generate",
        "db:format": "prisma format",
        "db:init-local": "prisma generate && prisma db push --skip-generate && pnpm run db:seed && prisma studio",
        "db:init": "pnpm install && prisma generate && prisma migrate deploy && prisma db push --skip-generate",
        "db:prod": "prisma migrate deploy",
        "db:studio": "prisma studio",
        "db:migrate": "prisma migrate dev",
        "db:postmigrate": "prisma generate",
        "db:seed": "ts-node seed.ts",
        "db:clean": "prisma migrate reset --force"
    },
    "dependencies": {
        "@faker-js/faker": "^8.4.1",
        "@prisma/client": "5.5.2"
    },
    "devDependencies": {
        "prisma": "5.5.2",
        "ts-node": "10.9.2"
    }
}